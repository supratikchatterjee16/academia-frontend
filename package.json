{
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "vitepress": "^1.6.3",
    "vitepress-sidebar": "^1.31.1"
  },
  "scripts": {
    "copy-robots": "copyfiles -u 1 ./public/robots.txt ./.vitepress/dist",
    "copy-icons": "copyfiles -u 1 ./public/*.png ./.vitepress/dist",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build && npm run copy-robots && npm run copy-icons",
    "docs:preview": "vitepress preview"
  },
  "dependencies": {
    "markdown-it-mathjax3": "^4.3.2"
  }
}
